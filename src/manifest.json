{
  "name": "Elemental New Tab",
  "description": "A Chrome extension that overrides the default new tab page with a minimalistic design of customizable links and background image.",
  "version": "0.2",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": ["storage","unlimitedStorage","<all_urls>"],
  "options_page": "options/options.html",
  "manifest_version": 2,
  "icons": {
    "16": "images/ico.png",
    "32": "images/ico.png",
    "48": "images/ico.png",
    "128": "images/ico.png"
  }
}
